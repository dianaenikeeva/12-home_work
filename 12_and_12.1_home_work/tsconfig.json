    "target": "es5",
    "module": "esnext",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
  },
  "include":[
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}